<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.slcc.jasonshepherd.hangdroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameActivity"></activity>
        <activity android:name=".GameOverActivity"></activity>
        <activity android:name=".MultiPlayerActivity"></activity>
        <activity android:name=".GameMultiActivity"></activity>
        <activity android:name=".ScoreActivity"></activity>
    </application>

</manifest>
